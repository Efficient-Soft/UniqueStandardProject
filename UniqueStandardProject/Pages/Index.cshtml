<script>
    document.addEventListener('DOMContentLoaded', function () {
        var url = 'api/products/masterList';
        var url1 = 'api/products/productDetail';

        fetch(url1).then((res) => res.json())
            .then((response) => {
                console.log(response);
                if (response.success) {
                    if (Array.isArray(response.data)) {
                        displayImage(response.data);
                    } else {
                        console.error('The data received is not an array.');
                    }
                } else {
                    console.error('Request was not successful.');
                }
            })
            .catch((error) => {
                console.error('Error fetching data:', error);
            });

        function displayImage(data) {
            var product = document.getElementById('project');
            var html = ``;
            data.forEach((player) => {
                html +=
                    `<div class="col-md-4">
                            <img src="${player.img}" style="width: 100%; height: 300px; object-fit: cover;">
                            <p class="name" style="text-color:primary; font-size: 20px;width: 100%;"><center>${player.title}</center></p>
                        </div>`;
            });
            product.innerHTML = html;
        }


        fetch(url)
            .then((res) => res.json())
            .then((response) => {
                console.log(response);
                if (response.success) {
                    if (Array.isArray(response.data)) {
                        displayData(response.data);
                    } else {
                        console.error('The data received is not an array.');
                    }
                } else {
                    console.error('Request was not successful.');
                }
            })
            .catch((error) => {
                console.error('Error fetching data:', error);
            });

        const displayData = (data) => {
            var mission = document.getElementById('mission');
            var html = ``;
            data.forEach((player) => {
                html +=
                    `<div class="col-md-12">
                            <h1 class="mb-5">Vision</h1>
                            <p class="name" style="color:primary; font-size: 20px;width: 100%;">${player.vision}</p>
                            <h1 class = "mb-5">Mission</h1>
                            <p class="name" style="color:primary; font-size: 20px;width: 100%;">${player.mission}</p>
                        </div>`;
            });
            if (mission) {
                mission.innerHTML = html;
            } else {
                console.error("Element not found or not loaded yet.");
            }

        };
    });


</script>


