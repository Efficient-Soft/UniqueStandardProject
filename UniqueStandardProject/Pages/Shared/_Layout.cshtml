    <nav class="navbar navbar-expand-lg bg-white navbar-light shadow sticky-top p-0">
        <a href="./Index" class="navbar-brand d-flex align-items-center ps-4">
            <img src="~/frontend/img/BunnerLogo.png" style="width:150px;" />
        </a>
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarCollapse" style="margin-right:50px;">
            <span class="navbar-toggler-icon"></span>
        </button>
        <div class="collapse navbar-collapse" id="navbarCollapse">
            <div class="navbar-nav ms-auto p-4 p-lg-0">
                <a asp-page="./Index" class="nav-item nav-link active text-success">Home</a>
                <a asp-page="./AboutUs" class="nav-item nav-link text-success">About Us</a>
                <a asp-page="./Service" class="nav-item nav-link text-success">Service</a>
                <a asp-page="./Activity" class="nav-item nav-link text-success">Activity</a>
                <div class="nav-item dropdown">
                    <a href="#" class="nav-link dropdown-toggle text-success" data-toggle="dropdown">Products</a>
