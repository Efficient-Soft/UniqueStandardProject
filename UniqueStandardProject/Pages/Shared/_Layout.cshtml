                        <a asp-page="./PremierCoatings" class="dropdown-item text-success">Premier Coatings</a>
                        <a asp-page="./HGG" class="dropdown-item text-success">HGG</a>
                        <a asp-page="./Olympus" class="dropdown-item text-success">Olympus</a>
                    </div>
                </div>
                <a asp-page="./WeldingTraining" class="nav-item nav-link text-success">WeldingTraining</a>
                <a asp-page="./Contact" class="nav-item nav-link text-success">Contact</a>
                <a asp-page="./Login" class="nav-link text-success">LOGIN</a>
            </div>
        </div>
    </nav>
    <!-- Navbar End -->
    @RenderBody()

    <!-- Footer Start -->
